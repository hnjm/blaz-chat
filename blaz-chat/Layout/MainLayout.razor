@inherits LayoutComponentBase

<MudThemeProvider Theme="_customTheme"/>
<MudDialogProvider/>
<MudSnackbarProvider/>

<MudLayout>
    @Body
</MudLayout>

@code{

    readonly MudTheme _customTheme = new()
    {
        Palette = new PaletteLight()
        {
            Primary = Colors.Teal.Darken1,
            Secondary = Colors.BlueGrey.Lighten1,
        },
    };

}